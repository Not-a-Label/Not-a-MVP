{
  "version": 2,
  "buildCommand": "cd not-a-label/frontend && npm run build",
  "outputDirectory": "not-a-label/frontend/.next",
  "framework": "nextjs",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "status": 404, "dest": "/404.html" }
  ],
  "github": {
    "silent": true
  },
  "domains": [
    "not-a-label.art",
    "www.not-a-label.art"
  ]
} 