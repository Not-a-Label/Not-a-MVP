{
  "version": 2,
  "builds": [
    { 
      "src": "not-a-label/frontend/package.json", 
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "not-a-label/frontend/$1",
      "continue": true
    },
    {
      "src": "/((?!api/.*).*)",
      "status": 404,
      "dest": "not-a-label/frontend/404.html"
    }
  ],
  "github": {
    "silent": true
  },
  "domains": [
    "not-a-label.art",
    "www.not-a-label.art"
  ]
} 